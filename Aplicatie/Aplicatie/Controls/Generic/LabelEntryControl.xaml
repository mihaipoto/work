<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Aplicatie.Controls.LabelEntryControl"
             x:Name="this">
    <HorizontalStackLayout>
        <Label Text="{Binding Cheie, Source={x:Reference this}}" />
        <Entry Placeholder="{Binding Placeholder,Source={x:Reference this}}"
               Text="{Binding ValoareCompletata,Source={x:Reference this}}"/>
        
    </HorizontalStackLayout>
</ContentView>
