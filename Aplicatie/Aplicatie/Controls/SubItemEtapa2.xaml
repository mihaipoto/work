<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Aplicatie.Controls.SubItemEtapa2"
             xmlns:controls="clr-namespace:Aplicatie.Controls"
             xmlns:viewmodel="clr-namespace:Aplicatie.ViewModels"
             x:DataType="viewmodel:SubItemEtapa2VM">
    <VerticalStackLayout>
        <controls:LabelLabelControl Cheie="Nume:"
                                    Valoare="{Binding SubItemNume}" />
        <controls:LabelLabelControl Cheie="Valoare:"
                                    Valoare="{Binding SubItemValoare}" />
    </VerticalStackLayout>
</ContentView>
